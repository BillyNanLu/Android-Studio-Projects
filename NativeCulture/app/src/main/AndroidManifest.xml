<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.NativeCulture"
        tools:targetApi="31">
        <activity
            android:name=".CultureActivity"
            android:exported="false"
            android:label="@string/category_culture"/>
        <activity
            android:name=".ProvincialismActivity"
            android:exported="false"
            android:label="@string/category_provincialism"/>
        <activity
            android:name=".ScenicActivity"
            android:exported="false"
            android:label="@string/category_scenic"/>
        <activity
            android:name=".FoodActivity"
            android:exported="false"
            android:label="@string/category_food"/>
        <activity
            android:name=".SelfActivity"
            android:exported="false"
            android:label="@string/category_self"/>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>